{
    "name": "jonathanmaron/image-optimizer",
    "description": "Image optimization / compression CLI tool. This tool optimizes PNG and JPEG files from the CLI, using 'pngout', 'pngcrush' and 'jpegtran'.",
    "keywords": [
        "image optimizer",
        "compression",
        "image",
        "optimization",
        "pngout",
        "pngcrush",
        "jpegtran"
    ],
    "type": "project",
    "homepage": "https://github.com/jonathanmaron/image-optimizer",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "Application\\": "src/Application"
        }
    },
    "require": {
        "php": "^7.1",
        "container-interop/container-interop": "^1.2",
        "symfony/config": "^4.0",
        "symfony/console": "^4.0",
        "symfony/filesystem": "^4.0",
        "symfony/finder": "^4.0",
        "symfony/lock": "^4.0",
        "symfony/yaml": "^4.0",
        "symfony/process": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/var-dumper": "^4.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit test"
    }
}
